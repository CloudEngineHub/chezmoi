# test that chezmoi apply rewrites the target path
exec chezmoi apply
! exists $HOME/.local/share/fonts/font.ttf
exists $HOME/Library/Fonts/font.ttf

-- home/user/.config/chezmoi/chezmoi.toml --
[rewriteTargetPaths]
    ".local/share/fonts" = "Library/Fonts"
